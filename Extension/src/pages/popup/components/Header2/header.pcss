.popup-header {
  display: flex;
  align-items: center;
  justify-content: space-between;
  min-height: 50px;
  padding: 0 16px;
  @media (prefers-color-scheme: dark) {
    background-color: var(--black);
  }

  @__logo {
    width: 113px;
    height: 25px;
    font-size: 28px;
    font-weight: bold;
  }
}
